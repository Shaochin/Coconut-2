<!-- Modal -->

<div class="panel-heading">
    <h3 class="panel-title text-center">請填寫課程名稱<br><small>Please complete full information</small></h3>
</div>
<div class="panel-body">
    <form role="form" method="post" action="/addNewCourse">
        <div class="row">
            <div class="col-xs-3 col-sm-3 col-md-3">
            </div>
            <div class="col-xs-6 col-sm-6 col-md-6">
                <input type="text" name="university" id="university" class="form-control input-sm" placeholder="大專院校">
            </div>
            <div class="col-xs-3 col-sm-3 col-md-3">
            </div>
        </div>
        <br>
        <div class="row">
            <div class="col-xs-3 col-sm-3 col-md-3">
            </div>
            <div class="col-xs-6 col-sm-6 col-md-6">
                <input type="text" name="uni_department" id="uni_department" class="form-control input-sm" placeholder="系級">
            </div>
            <div class="col-xs-3 col-sm-3 col-md-3">
            </div>
        </div>
        <br>
        <input type="hidden" name="ownerFBuid" value=<%=CourseData.ownerFBuid %> >

        <input type="submit" value="Register" class="btn btn-info btn-block">
    </form>
</div>
